{
  "id": "subfolder-indent",
  "title": "Subfolder Indent",
  "description": "<p>A simple module that increases the size of the indent of subfolders</p>",
  "version": "#{VERSION}#",
  "library": "false",
  "manifestPlusVersion": "1.2.1",
  "compatibility": {
    "minimum": 10,
    "verified": 12.324
  },
  "authors": [
    {
    "name": "Mushie",
    "discord": "Discord Mushie#6848"
    }
  ],
 "media": [
        {
  	"type": "cover",
        "url": "modules/subfolder-indent/examples/example-on.jpg" 
        },
    	  {
        "type": "screenshot",
        "url": "modules/subfolder-indent/examples/example-off.jpg"
        },
        {
        "type": "screenshot",
        "url": "modules/subfolder-indent/examples/settings.jpg"
        }
  ],
  "esmodules": [
    "./scripts/settings.js"
  ],
  "styles": [
    "./styles/style.css"
  ],
   "config": {
    "subfolder-indent": {
      "name": "Indent",
      "hint": "Set the amount to index each subfolder in pixels.",
      "scope": "world",
      "config": true,
      "default": 12,
      "type": "Number"
    }
    },
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "license": "LICENSE",
  "readme": "README.md"
}
